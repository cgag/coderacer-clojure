p.=''       /*a short list of primes.*/
p.1=2; p.2=3; p.3=5; p.4=7; p.5=11; p.6=13; p.7=17; p.8=19; p.9=23
p.10=27; p.11=31; p.12=37

f.=''       /*a short list of Fibonacci numbers.*/
f.1=1;f.2=1;f.3=2;f.4=3;f.5=5;f.6=8;f.7=13;f.8=21;f.9=34;f.10=55

n=0

  do j=1 while p.j\==''
  n=n+1
  c.n=p.j
  end

    do k=1 while f.k\==''
    n=n+1
    c.n=f.k
    end

say 'n'=n

  do m=1 for n
  say 'c.'m"="c.m
  end