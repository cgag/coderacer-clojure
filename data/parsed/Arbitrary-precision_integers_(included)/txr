@(bind (f20 l20 ndig) 
       @(let* ((num (expt 5 4 3 2))
               (str (format nil "~s" num))
               (len (length str)))
          '(,(sub-str str 0 20) ,(sub-str str (- len 20) len) ,len)))
@(bind f20 "62060698786608744707")
@(bind l20 "92256259918212890625")
@(output)
@f20...@l20
ndigits=@ndig
@(end)